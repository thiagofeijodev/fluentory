{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2016",
    "baseUrl": "src",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/contexts/*": ["src/data/*"],
      "@/functions/*": ["src/functions/*"],
      "@/hooks/*": ["src/hooks/*"]
    }
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
